{
    "name": "dazzlesoftware/oauth1",
    "type": "library",
    "description": "Dazzle OAuth1 Package",
    "keywords": ["dazzle", "framework", "oauth1"],
    "homepage": "https://github.com/dazzle-framework/oauth1",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "dazzlesoftware/application": "^1.0",
        "dazzlesoftware/http": "^1.0",
        "dazzlesoftware/input": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "dazzlesoftware/session": "^1.0",
        "dazzlesoftware/uri": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "dazzlesoftware/event": "^1.0",
        "dazzlesoftware/test": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\OAuth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\OAuth1\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
